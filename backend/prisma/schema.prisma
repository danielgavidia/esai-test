generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Card {
  // Defaults
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())

  // Fields
  title   String @default("")
  content String @default("")
  type    String @default("story-strategist")
}
